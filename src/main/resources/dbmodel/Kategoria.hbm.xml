<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dbmodel.Kategoria" table="KATEGORIE">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="nazwa" type="string">
            <column name="nazwa" length="255" not-null="true" unique="true" />
        </property>
        
        <set name="dania" table="DANIA">
            <key column="id"></key>
            <composite-element class="dbmodel.Danie">
                <property name="nazwa" type="string">
                    <column name="nazwa" length="255" not-null="true" unique="true" />
                </property>
                <property name="cena" type="double">
                    <column name="cena" length="25" not-null="true" unique="false" />
                </property>
                <property name="kategoria" type="dbmodel.Kategoria">
                    <column name="kategoria" length="255" not-null="true" unique="true" />
                </property>
                <property name="zdjecie" type="string">
                    <column name="zdjecie" length="255" not-null="true" unique="true" />
                </property>
            </composite-element>
        </set>
    </class>
</hibernate-mapping>